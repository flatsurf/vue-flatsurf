{
  "name": "vue-flatsurf",
  "version": "0.10.1",
  "description": "",
  "scripts": {
    "serve": "vite",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:es:dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "yarn mochapack --recursive --require test/jsdom.mock.js --webpack-config node_modules/@vue/cli-service/webpack.config.js 'test/**/*.test.ts'"
  },
  "main": "dist/vue-flatsurf.ssr.js",
  "module": "dist/vue-flatsurf.esm.js",
  "browser": "dist/vue-flatsurf.esm.js",
  "unpkg": "dist/vue-flatsurf.min.js",
  "files": [
    "dist/*",
    "vue-flatsurf.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {
    "@flatten-js/core": "^1.2.24",
    "@mdi/js": "^6.4.95",
    "@thesoulfresh/pan-zoom": "^4.1.3",
    "color-normalize": "^1.5.2",
    "iwanthue": "^1.4.2",
    "lodash-es": "^4.17.15",
    "parsimmon": "^1.18.1",
    "svgo": "^3.0.2",
    "vue-async-computed": "^3.0.0"
  },
  "devDependencies": {
    "@mdi/font": "^6.3.95",
    "@types/chai": "^4.2.20",
    "@types/chai-as-promised": "^7.1.4",
    "@types/lodash-es": "^4.17.3",
    "@types/mocha": "^8.2.3",
    "@types/parsimmon": "^1.10.6",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/sinon": "^10.0.2",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/test-utils": "^1",
    "cross-env": "^7.0.2",
    "deepmerge": "^4.2.2",
    "jsdom": "^21.1.0",
    "minimist": "^1.2.5",
    "postcss": "^8.3.5",
    "process": "^0.11.10",
    "resize-observer-polyfill": "^1.5.1",
